<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8"/>
<script type="application/javascript">
function draw() {
  var ctx = document.getElementById('canvas').getContext('2d');
  for (var i=0;i<8;i++){
    for (var j=0;j<6;j++){
      ctx.fillStyle = '#FF0000'
      ctx.fillRect(j*25,i*25,25,25);
    }
  }
}
</script>
</head>
<body onload="draw();">
<canvas id="canvas" width="200" height="150"></canvas>
</body>
</html>

